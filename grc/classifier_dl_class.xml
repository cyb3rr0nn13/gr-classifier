<?xml version="1.0"?>
<block>
  <name>dl_class</name>
  <key>classifier_dl_class</key>
  <category>[CLASSIFIER]</category>
  <import>import classifier</import>
  <make>classifier.dl_class($modelfile)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
 <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <param>
  	<name>Model file</name>
  	<key>modelfile</key>
  	<value></value>
  	<type>file_open</type>
  </param>

  <sink>
    <name>in</name>
    <type>float</type>
    <vlen>64</vlen>
  </sink>

  <source>
    <name>pred_prob</name>
    <type>float</type>
    <vlen>10</vlen>
  </source>

  <source>
    <name>scn</name>
    <type>float</type>
    <vlen>10</vlen>
  </source>
</block>
